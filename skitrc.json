{
  "name": "navigator",
  "type": "report",
  "menu": {
    "path": "/navigator/index",
    "name": "navigator",
    "title": "导航",
    "icon": "tab"
  },
  "containers": {
    "searchBar": [{
        "type": "input",
        "key": "phonenum",
        "label": "手机号",
        "show": true,
        "span": 1,
        "defValue": {
          "value": ""
        },
        "placeholder": "请输入手机号",
        "props": {
          "size": "mini"
        }
      },
      {
        "type": "input",
        "key": "nickname",
        "label": "昵称",
        "show": true,
        "span": 1,
        "defValue": {
          "value": ""
        },
        "placeholder": "请输入昵称",
        "props": {
          "size": "mini"
        }
      },
      {
        "type": "select",
        "key": "gender",
        "label": "性别",
        "show": true,
        "span": 1,
        "defValue": {
          "value": ""
        },
        "placeholder": "请选择性别",
        "props": {
          "size": "mini"
        },
        "dataList": [
          {
            "label": "男",
            "value": 1
          },
          {
            "label": "女",
            "value": 0
          }
        ]
      },
      {
        "type": "date-range",
        "key": "timeRange",
        "label": "时间范围",
        "show": true,
        "span": 2,
        "defValue": {
          "value": []
        },
        "placeholder": "请输入",
        "props": {
          "type": "datetimerange",
          "range-separator": "至",
          "start-placeholder": "开始日期",
          "end-placeholder": "结束日期",
          "clearable": false,
          "value-format": "yyyy-MM-dd HH:mm:ss",
          "size": "mini"
        }
      }
    ],
    "tableConfig":{
      "fieldsArr": [{
          "key": "nickname",
          "name": "昵称",
          "show": true,
          "width": 200
        },
        {
          "key": "phonenum",
          "name": "手机号",
          "show": true
        },
        {
          "key": "gender",
          "name": "性别",
          "show": true
        }
      ],
      "formItems": [{
        "type": "button",
        "name": "导出",
        "btype": "primary",
        "feature": "export"
      }],
      "selectType": "multiple",
      "immediateQuery": true,
      "showActionCol": true,
      "showIndexCol": true,
      "pagination": true,
      "pagePosition": "up",
      "actionColLabel": "操作",
      "actionBtn": [{
          "type": "btn",
          "name": "点击大按钮",
          "btype": "danger"
        },
        {
          "type": "a",
          "name": "大链接"
        }
      ]
    }
  },
  "feature": {
    "config": {
      "prefix": "user",
      "items": [
        { "key": "queryList", "callee": "bbbb", "url": "/query" },
        { "key": "exportList", "callee": "exportList", "url": "/export" }
      ]
    }
  }
}


